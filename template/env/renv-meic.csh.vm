setenv emissiondir  ${emissiondir}

setenv taskId ${taskId}

setenv max_dom ${max_dom}

if( ${meic_type} == cache) then

    #conf_template /public/home/tsinghua/wuguanyu/model-driver/template/meicd0${domain}_${meic_type}.conf.template
    setenv conf_template ${conf_template}

    setenv scripts_path ${scripts_path}

    setenv meic_build_path ${meic_build_path}

    setenv invoke_file ${invoke_file}

else if (${meic_type} == server) then

    #conf_template /public/home/tsinghua/wuguanyu/model-driver/template/meicd0${domain}_${meic_type}.conf.template

    setenv conf_template ${conf_template}

    setenv onefilehours ${onefilehours}

